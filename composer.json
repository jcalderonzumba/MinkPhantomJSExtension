{
  "name": "jcalderonzumba/behat-phantomjs-extension",
  "description": "PhantomJS Mink Behat extension for Phantomjs Mink driver",
  "keywords": [
    "phantomjs",
    "headless",
    "javascript",
    "ajax",
    "testing",
    "browser"
  ],
  "homepage": "https://github.com/jcalderonzumba/MinkPhantomJSExtension",
  "type": "behat-extension",
  "license": "MIT",
  "authors": [
    {
      "name": "Juan Francisco Calderon Zumba",
      "email": "juanfcz@gmail.com",
      "homepage": "http://github.com/jcalderonzumba"
    }
  ],
  "require": {
    "php": ">=5.4",
    "behat/mink-extension": "~2.0",
    "jcalderonzumba/mink-phantomjs-driver" : "^0.4@dev"
  },
  "config": {
    "bin-dir": "bin"
  },
  "autoload": {
    "psr-4": {
      "Zumba\\PhantomJSExtension\\": "src"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.1.x-dev"
    }
  }
}
